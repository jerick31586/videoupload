@using PagedList
@using PagedList.Mvc

@model VideoUpload.Core.Entities.Post

@foreach (var item in Model)
{
    @Html.Partial("_Post", item)
}

@Html.PagedListPager(Model, page => Url.Action("posts", "videos", new { page, v = Request.QueryString["v"] }), PagedListRenderOptions.TwitterBootstrapPagerAligned)