<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  
  <link rel="shortcut icon" href="~/dist/images/favicon.ico" />
  <title>PCD - After Sales | @ViewBag.Title</title>

    <!-- build:css /css/main.min.css-->
  <link href="~/assets/css/app.css" rel="stylesheet" />
    <!-- endbuild -->
</head>
<body>

  @Html.Partial("_MainHeader")

  <div class="container">
    <div id="marquee-holder">      
      <img src="~/dist/images/porsche-marque-rgb-digital_M_R.jpg" alt="Porsche" />
    </div>
  </div>

  @RenderBody()
  
  @Html.Partial("_Footer")

  <!-- build:js /js/main.min.js -->
  <script src="~/dist/js/jquery.min.js"></script>
  <script src="~/dist/js/bootstrap.min.js"></script>
  <script src="~/dist/js/jquery.cookie.js"></script>
  <script src="https://cloud.tinymce.com/stable/tinymce.min.js?apiKey=5p70a8d0ew9m48r9xmcngtz3t0itd0czyhvrsiimy476qizj"></script>
  <script src="~/assets/js/videos/timeZoneCookie.js"></script>

  <!-- endbuild -->
  @RenderSection("Script", required: false)

</body>
</html>
